{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"baseUrl": "./",
		"module": "ESNext",
		"moduleResolution": "node",
		"paths": { "@/*": ["./src/*"] },
		"types": ["vite/client", "unplugin-icons/types/solid"],
		"noEmit": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true
	}
}
